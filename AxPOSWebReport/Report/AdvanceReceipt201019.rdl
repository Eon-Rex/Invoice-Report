        <CommandText>Select ACXCUSTTABLE.PANNUMBER,ACXRETAILTRANSACTIONTABLE.CUSTNAME,ACXRETAILTRANSACTIONTABLE.CUSTMOBILE,ACXRETAILTRANSACTIONPAYMENTTRANS.TenderType,

ACXCUSTOMERADVANCE.AMOUNT,ACXRETAILTRANSACTIONSALESTRANS.ITEMID,ACXCUSTOMERADVANCE.RATEG100,ACXRETAILTRANSACTIONSALESTRANS.SALESPERSONCODE,

ACXCUSTOMERADVANCE.ISRATEFIXED,ACXCUSTOMERADVANCE.ADVANCEEXPIRYDATE,ACXCUSTOMERADVANCE.ADVANCETYPE

 From ACXCUSTOMERADVANCE,ACXCUSTTABLE,ACXRETAILTRANSACTIONSALESTRANS,ACXRETAILTRANSACTIONTABLE,ACXRETAILTRANSACTIONPAYMENTTRANS where

ACXRETAILTRANSACTIONSALESTRANS.TRANSACTIONID=ACXRETAILTRANSACTIONTABLE.TRANSACTIONID and ACXCUSTTABLE.CUSTACCOUNT=ACXCUSTOMERADVANCE.CUSTOMERCODE   and

ACXRETAILTRANSACTIONPAYMENTTRANS.TRANSACTIONID=ACXCUSTOMERADVANCE.TRANSACTIONID and ACXRETAILTRANSACTIONPAYMENTTRANS.CUSTACCOUNT=ACXCUSTOMERADVANCE.CUSTOMERCODE

and

